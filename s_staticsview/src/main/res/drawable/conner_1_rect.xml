<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/normal_txt_color_white"/>
            <stroke android:width="@dimen/dp_1" android:color="@color/normal_txt_color_cyan"/>
        </shape>
    </item >
    <item android:state_selected="false" android:state_pressed="false">
        <shape>
            <solid android:color="@color/normal_txt_color_white"/>
            <stroke android:width="@dimen/dp_1" android:color="@color/normal_title_color"/>
        </shape>
    </item >
</selector>